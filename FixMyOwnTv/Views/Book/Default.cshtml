        function ToggleStars(rating) {
            if ($('#StarRatingThankYou').css('visibility') === 'hidden') {
                $('.fa').removeClass('checked');
                $('#StarRatingThankYou').css('visibility', 'visible');
                switch (rating) {
                    case 5:
                        $('#star5').addClass('checked');
                        $('#star4').addClass('checked');
                        $('#star3').addClass('checked');
                        $('#star2').addClass('checked');
                        $('#star1').addClass('checked');
                        break;
                    case 4:
                        $('#star4').addClass('checked');
                        $('#star3').addClass('checked');
                        $('#star2').addClass('checked');
                        $('#star1').addClass('checked');
                        break;
                    case 3:
                        $('#star3').addClass('checked');
                        $('#star2').addClass('checked');
                        $('#star1').addClass('checked');
                        break;
                    case 2:
                        $('#star2').addClass('checked');
                        $('#star1').addClass('checked');
                        break;
                    case 1:
                        $('#star1').addClass('checked');
                        break;
                }

                $.ajax({
                    type: "GET",
                    url: 'http://' + '@ViewBag.Host' + '/API/Rating?ThisRating=' + rating,
                    contentType: "application/json; charset=utf-8",
                    async: true,
                    complete: function (msg) {
                        $('#Content').html(msg.responseText);
                    }
                });

            }
        }

